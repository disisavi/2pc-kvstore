<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="edu.gmu.cs675.master.model">
    <class name="TransactionLogger" table="transactionLoggers">
        <id name="transactionId" type="java.lang.Integer"/>
        <property name="key" type="string" length="100" not-null="false"/>
        <property name="Value" type="string" length="100" not-null="false"/>
        <property name="state" type="integer" length="100" not-null="false"/>
        <property name="operation" type="java.lang.String" length="100" not-null="true"/>
    </class>
</hibernate-mapping>